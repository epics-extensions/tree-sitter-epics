record(event, record_name) {
    alias(alias_name)
    field(field_name, "field_value")
    field(INP, {"const": "Inf"})
    field(INP, other)
    field(FLNK, hello)
    field(OUT, hello)
    info(info_name, "info_value")
}
alias(record_name, alias_name)

alias(hello, world)
alias(world, bla)
